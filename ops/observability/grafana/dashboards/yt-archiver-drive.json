{
  "uid": "yt-archiver-drive",
  "title": "YT-Archiver - Drive Interactions",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 2,
  "refresh": "10s",
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "RPS (Drive)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(rate(yt_archiver_http_requests_total{path=~\"^/api/drive.*\"}[$__rate_interval]))",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "req/s" }, "overrides": [] },
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Error Rate (5xx)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "100 * sum(rate(yt_archiver_http_requests_total{path=~\"^/api/drive.*\",status=~\"5..\"}[$__rate_interval])) / sum(rate(yt_archiver_http_requests_total{path=~\"^/api/drive.*\"}[$__rate_interval]))",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "percent" }, "overrides": [] },
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 0 }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Latency p95",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(yt_archiver_http_request_duration_seconds_bucket{path=~\"^/api/drive.*\"}[$__rate_interval])) by (le))",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 0 }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Success %",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "100 * (1 - (sum(rate(yt_archiver_http_requests_total{path=~\"^/api/drive.*\",status=~\"5..\"}[$__rate_interval])) / sum(rate(yt_archiver_http_requests_total{path=~\"^/api/drive.*\"}[$__rate_interval]))))",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "percent" }, "overrides": [] },
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 0 }
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "RPS by Operation",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(rate(yt_archiver_http_requests_total{path=~\"^/api/drive/sync.*\"}[$__rate_interval]))",
          "legendFormat": "sync",
          "refId": "A"
        },
        {
          "expr": "sum(rate(yt_archiver_http_requests_total{path=~\"^/api/drive/upload.*\"}[$__rate_interval]))",
          "legendFormat": "upload",
          "refId": "B"
        },
        {
          "expr": "sum(rate(yt_archiver_http_requests_total{path=~\"^/api/drive/download.*\"}[$__rate_interval]))",
          "legendFormat": "download",
          "refId": "C"
        },
        {
          "expr": "sum(rate(yt_archiver_http_requests_total{path=\"/api/drive/videos\"}[$__rate_interval]))",
          "legendFormat": "list",
          "refId": "D"
        },
        {
          "expr": "sum(rate(yt_archiver_http_requests_total{path=~\"^/api/drive/stream/.*\"}[$__rate_interval]))",
          "legendFormat": "stream",
          "refId": "E"
        },
        {
          "expr": "sum(rate(yt_archiver_http_requests_total{path=~\"^/api/drive/(thumbnail|custom-thumbnail)/.*\"}[$__rate_interval]))",
          "legendFormat": "thumbnail",
          "refId": "F"
        },
        {
          "expr": "sum(rate(yt_archiver_http_requests_total{path=~\"^/api/drive/(auth-status|auth-url|oauth2callback)$\"}[$__rate_interval]))",
          "legendFormat": "auth",
          "refId": "G"
        },
        {
          "expr": "sum(rate(yt_archiver_http_requests_total{path=~\"^/api/drive/videos/[^/]+/(rename|thumbnail|share)$|^/api/drive/videos/delete-batch$\"}[$__rate_interval]))",
          "legendFormat": "mutations",
          "refId": "H"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "req/s" }, "overrides": [] },
      "options": { "legend": { "displayMode": "table" }, "stacking": { "mode": "normal" } },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 }
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Latency p95 by Operation",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(yt_archiver_http_request_duration_seconds_bucket{path=~\"^/api/drive/sync.*\"}[$__rate_interval])) by (le))",
          "legendFormat": "sync",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(yt_archiver_http_request_duration_seconds_bucket{path=~\"^/api/drive/upload.*\"}[$__rate_interval])) by (le))",
          "legendFormat": "upload",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(yt_archiver_http_request_duration_seconds_bucket{path=~\"^/api/drive/download.*\"}[$__rate_interval])) by (le))",
          "legendFormat": "download",
          "refId": "C"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(yt_archiver_http_request_duration_seconds_bucket{path=\"/api/drive/videos\"}[$__rate_interval])) by (le))",
          "legendFormat": "list",
          "refId": "D"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 }
    },
    {
      "id": 7,
      "type": "table",
      "title": "Top Drive Endpoints (RPS)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "topk(10, sum by (path) (rate(yt_archiver_http_requests_total{path=~\"^/api/drive.*\"}[5m])))",
          "format": "table",
          "instant": true,
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "req/s" }, "overrides": [] },
      "gridPos": { "h": 6, "w": 24, "x": 0, "y": 12 }
    }
  ]
}
