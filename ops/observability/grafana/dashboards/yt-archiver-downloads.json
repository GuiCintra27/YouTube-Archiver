{
  "uid": "yt-archiver-downloads",
  "title": "YT-Archiver - Downloads",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Download Start RPS",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(rate(yt_archiver_http_requests_total{path=\"/api/download\",method=\"POST\"}[$__rate_interval]))",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "req/s" }, "overrides": [] },
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Video Info RPS",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(rate(yt_archiver_http_requests_total{path=\"/api/video-info\",method=\"POST\"}[$__rate_interval]))",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "req/s" }, "overrides": [] },
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 0 }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Download Error Rate (5xx)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "100 * sum(rate(yt_archiver_http_requests_total{path=\"/api/download\",status=~\"5..\"}[$__rate_interval])) / sum(rate(yt_archiver_http_requests_total{path=\"/api/download\"}[$__rate_interval]))",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "percent" }, "overrides": [] },
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 0 }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Download Latency p95",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(yt_archiver_http_request_duration_seconds_bucket{path=\"/api/download\"}[$__rate_interval])) by (le))",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 0 }
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "RPS by Endpoint",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(rate(yt_archiver_http_requests_total{path=\"/api/download\"}[$__rate_interval]))",
          "legendFormat": "download",
          "refId": "A"
        },
        {
          "expr": "sum(rate(yt_archiver_http_requests_total{path=\"/api/video-info\"}[$__rate_interval]))",
          "legendFormat": "video-info",
          "refId": "B"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "req/s" }, "overrides": [] },
      "options": { "legend": { "displayMode": "table" }, "stacking": { "mode": "normal" } },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 }
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Download Latency (p50 / p95 / p99)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(yt_archiver_http_request_duration_seconds_bucket{path=\"/api/download\"}[$__rate_interval])) by (le))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(yt_archiver_http_request_duration_seconds_bucket{path=\"/api/download\"}[$__rate_interval])) by (le))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(yt_archiver_http_request_duration_seconds_bucket{path=\"/api/download\"}[$__rate_interval])) by (le))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 }
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "Video Info Latency (p50 / p95)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(yt_archiver_http_request_duration_seconds_bucket{path=\"/api/video-info\"}[$__rate_interval])) by (le))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(yt_archiver_http_request_duration_seconds_bucket{path=\"/api/video-info\"}[$__rate_interval])) by (le))",
          "legendFormat": "p95",
          "refId": "B"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 }
    },
    {
      "id": 8,
      "type": "table",
      "title": "Download HTTP Status Codes",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum by (status) (rate(yt_archiver_http_requests_total{path=\"/api/download\"}[5m]))",
          "format": "table",
          "instant": true,
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "req/s" }, "overrides": [] },
      "gridPos": { "h": 6, "w": 12, "x": 12, "y": 12 }
    }
  ]
}
