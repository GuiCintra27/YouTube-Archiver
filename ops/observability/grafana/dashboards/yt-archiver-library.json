{
  "uid": "yt-archiver-library",
  "title": "YT-Archiver - Library",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "RPS",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(rate(yt_archiver_http_requests_total{path=~\"^/api/videos.*\"}[$__rate_interval]))",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "req/s" }, "overrides": [] },
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Error Rate (5xx)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "100 * sum(rate(yt_archiver_http_requests_total{path=~\"^/api/videos.*\",status=~\"5..\"}[$__rate_interval])) / sum(rate(yt_archiver_http_requests_total{path=~\"^/api/videos.*\"}[$__rate_interval]))",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "percent" }, "overrides": [] },
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 0 }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Latency p95",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(yt_archiver_http_request_duration_seconds_bucket{path=~\"^/api/videos.*\"}[$__rate_interval])) by (le))",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 0 }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Success %",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "100 * (1 - (sum(rate(yt_archiver_http_requests_total{path=~\"^/api/videos.*\",status=~\"5..\"}[$__rate_interval])) / sum(rate(yt_archiver_http_requests_total{path=~\"^/api/videos.*\"}[$__rate_interval]))))",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "percent" }, "overrides": [] },
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 0 }
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Requests per Second",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(rate(yt_archiver_http_requests_total{path=~\"^/api/videos.*\"}[$__rate_interval]))",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "req/s" }, "overrides": [] },
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 4 }
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Latency (p50 / p95 / p99)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(yt_archiver_http_request_duration_seconds_bucket{path=~\"^/api/videos.*\"}[$__rate_interval])) by (le))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(yt_archiver_http_request_duration_seconds_bucket{path=~\"^/api/videos.*\"}[$__rate_interval])) by (le))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(yt_archiver_http_request_duration_seconds_bucket{path=~\"^/api/videos.*\"}[$__rate_interval])) by (le))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 4 }
    },
    {
      "id": 7,
      "type": "table",
      "title": "Top Endpoints (RPS)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "topk(10, sum by (path) (rate(yt_archiver_http_requests_total{path=~\"^/api/videos.*\"}[5m])))",
          "format": "table",
          "instant": true,
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "req/s" }, "overrides": [] },
      "gridPos": { "h": 7, "w": 24, "x": 0, "y": 11 }
    }
  ]
}
