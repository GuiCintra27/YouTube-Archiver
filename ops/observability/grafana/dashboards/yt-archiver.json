{
  "uid": "yt-archiver",
  "title": "YT-Archiver - Backend Observability",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 3,
  "refresh": "10s",
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "method",
        "type": "query",
        "datasource": "Prometheus",
        "query": "label_values(yt_archiver_http_requests_total, method)",
        "multi": true,
        "includeAll": true,
        "allValue": ".*",
        "refresh": 1,
        "sort": 1
      },
      {
        "name": "path",
        "type": "query",
        "datasource": "Prometheus",
        "query": "label_values(yt_archiver_http_requests_total, path)",
        "multi": true,
        "includeAll": true,
        "allValue": ".*",
        "refresh": 1,
        "sort": 1
      },
      {
        "name": "status",
        "type": "query",
        "datasource": "Prometheus",
        "query": "label_values(yt_archiver_http_requests_total, status)",
        "multi": true,
        "includeAll": true,
        "allValue": ".*",
        "refresh": 1,
        "sort": 1
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "row",
      "title": "Overview",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Backend Up",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "max(up{job=\"yt-archiver-backend\"})",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "type": "value", "options": { "0": { "text": "Down" }, "1": { "text": "Up" } } }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": 0 },
              { "color": "green", "value": 1 }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 1 }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "RPS",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(rate(yt_archiver_http_requests_total{method=~\"$method\",path=~\"$path\",status=~\"$status\"}[$__rate_interval]))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "req/s"
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 1 }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Error Rate (5xx)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "100 * sum(rate(yt_archiver_http_requests_total{status=~\"5..\",method=~\"$method\",path=~\"$path\"}[$__rate_interval])) / sum(rate(yt_archiver_http_requests_total{method=~\"$method\",path=~\"$path\"}[$__rate_interval]))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": 0 },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 5 }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 1 }
    },
    {
      "id": 5,
      "type": "stat",
      "title": "Latency p95",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(yt_archiver_http_request_duration_seconds_bucket{method=~\"$method\",path=~\"$path\"}[$__rate_interval])) by (le))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 3
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 1 }
    },
    {
      "id": 6,
      "type": "row",
      "title": "SLO / SLA",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 }
    },
    {
      "id": 7,
      "type": "stat",
      "title": "Availability (Success %)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "100 * (1 - (sum(rate(yt_archiver_http_requests_total{status=~\"5..\",method=~\"$method\",path=~\"$path\"}[$__rate_interval])) / sum(rate(yt_archiver_http_requests_total{method=~\"$method\",path=~\"$path\"}[$__rate_interval]))))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 3,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": 0 },
              { "color": "yellow", "value": 99 },
              { "color": "green", "value": 99.9 }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 8, "x": 0, "y": 6 }
    },
    {
      "id": 8,
      "type": "stat",
      "title": "Latency SLO (<= 1s)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "100 * sum(rate(yt_archiver_http_request_duration_seconds_bucket{le=\"1\",method=~\"$method\",path=~\"$path\"}[$__rate_interval])) / sum(rate(yt_archiver_http_request_duration_seconds_count{method=~\"$method\",path=~\"$path\"}[$__rate_interval]))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 3,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": 0 },
              { "color": "yellow", "value": 95 },
              { "color": "green", "value": 99 }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 8, "x": 8, "y": 6 }
    },
    {
      "id": 9,
      "type": "stat",
      "title": "Error Budget Burn (99% SLO)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "(sum(rate(yt_archiver_http_requests_total{status=~\"5..\",method=~\"$method\",path=~\"$path\"}[$__rate_interval])) / sum(rate(yt_archiver_http_requests_total{method=~\"$method\",path=~\"$path\"}[$__rate_interval]))) / 0.01",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "x",
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": 0 },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 2 }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": { "h": 4, "w": 8, "x": 16, "y": 6 }
    },
    {
      "id": 10,
      "type": "timeseries",
      "title": "Requests per Second",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(rate(yt_archiver_http_requests_total{method=~\"$method\",path=~\"$path\",status=~\"$status\"}[$__rate_interval]))",
          "legendFormat": "total",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "req/s"
        },
        "overrides": []
      },
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 10 }
    },
    {
      "id": 11,
      "type": "timeseries",
      "title": "Latency (p50 / p95 / p99)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(yt_archiver_http_request_duration_seconds_bucket{method=~\"$method\",path=~\"$path\"}[$__rate_interval])) by (le))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(yt_archiver_http_request_duration_seconds_bucket{method=~\"$method\",path=~\"$path\"}[$__rate_interval])) by (le))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(yt_archiver_http_request_duration_seconds_bucket{method=~\"$method\",path=~\"$path\"}[$__rate_interval])) by (le))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      },
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 10 }
    },
    {
      "id": 12,
      "type": "timeseries",
      "title": "Requests by Status",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum by (status) (rate(yt_archiver_http_requests_total{method=~\"$method\",path=~\"$path\"}[$__rate_interval]))",
          "legendFormat": "{{status}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "req/s"
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table" },
        "stacking": { "mode": "normal" }
      },
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 17 }
    },
    {
      "id": 13,
      "type": "timeseries",
      "title": "Requests by Method",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum by (method) (rate(yt_archiver_http_requests_total{path=~\"$path\",status=~\"$status\"}[$__rate_interval]))",
          "legendFormat": "{{method}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "req/s"
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table" },
        "stacking": { "mode": "normal" }
      },
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 17 }
    },
    {
      "id": 14,
      "type": "table",
      "title": "Top Endpoints (RPS)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "topk(10, sum by (path) (rate(yt_archiver_http_requests_total{method=~\"$method\",status=~\"$status\"}[5m])))",
          "format": "table",
          "instant": true,
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "req/s"
        },
        "overrides": []
      },
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 24 }
    },
    {
      "id": 15,
      "type": "table",
      "title": "Slowest Endpoints (p95)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "topk(10, histogram_quantile(0.95, sum(rate(yt_archiver_http_request_duration_seconds_bucket{method=~\"$method\"}[5m])) by (le, path)))",
          "format": "table",
          "instant": true,
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 3
        },
        "overrides": []
      },
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 24 }
    },
    {
      "id": 16,
      "type": "heatmap",
      "title": "Latency Histogram",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(rate(yt_archiver_http_request_duration_seconds_bucket{method=~\"$method\",path=~\"$path\"}[$__rate_interval])) by (le)",
          "refId": "A"
        }
      ],
      "gridPos": { "h": 7, "w": 24, "x": 0, "y": 31 }
    }
  ]
}
